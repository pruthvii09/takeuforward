generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Submission {
  id                Int       @id @default(autoincrement())
  username          String
  language_id       Int
  stdin             String
  source_code       String 
  timestamp         DateTime  @default(now())
}